<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="SpeakableTextPresentCheck">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="421dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.24"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@color/grey">

                <androidx.cardview.widget.CardView
                    android:id="@+id/planifie"
                    android:layout_width="204dp"
                    android:layout_height="match_parent"
                    android:background="@color/grey">


                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="match_parent"
                        android:layout_height="43dp"
                        android:background="@drawable/background"
                        android:gravity="center"
                        android:text="Planifiée"
                        android:textColor="#FFFFFF"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/heure12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView2"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="60dp"
                        android:text="Heures : "
                        android:textSize="20sp"
                        app:backgroundTint="@color/purple_200"
                        app:cornerRadius="20dp" />

                    <TextView
                        android:id="@+id/textviewtravailler"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView2"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="100dp"
                        android:text="Temps Travailler: 00:00 "
                        android:textSize="20sp"
                        app:backgroundTint="@color/purple_200"
                        app:cornerRadius="20dp" />

                    <TextView
                        android:id="@+id/textviewactions"
                        android:layout_width="wrap_content"
                        android:layout_height="56dp"
                        android:layout_below="@+id/imageView2"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="150dp"
                        android:text="Actions: Element 1 "
                        android:textSize="20sp"
                        app:backgroundTint="@color/purple_200"
                        app:cornerRadius="20dp" />

                    <TextView
                        android:id="@+id/textviewcommentaires"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView2"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="190dp"
                        android:text="Commentaires : vide "
                        android:textSize="20sp"
                        app:backgroundTint="@color/purple_200"
                        app:cornerRadius="20dp" />

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/InterventionEffectué"
                    android:layout_width="203dp"
                    android:layout_height="match_parent"
                    android:background="@color/grey">

                    <TextView
                        android:id="@+id/textVieweffectue"
                        android:layout_width="match_parent"
                        android:layout_height="43dp"
                        android:background="@drawable/background"
                        android:gravity="center"
                        android:text="Effectué"
                        android:textColor="#FFFFFF"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/textviewtermine"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView2"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="60dp"
                        android:text="Terminé : oui "
                        android:textSize="20sp"
                        app:backgroundTint="@color/purple_200"
                        app:cornerRadius="20dp" />

                    <TextView
                        android:id="@+id/textviewtravailler2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView2"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="100dp"
                        android:text="Temps Travailler: 00:00 "
                        android:textSize="20sp"
                        app:backgroundTint="@color/purple_200"
                        app:cornerRadius="20dp" />

                    <TextView
                        android:id="@+id/textviewactions2"
                        android:layout_width="wrap_content"
                        android:layout_height="56dp"
                        android:layout_below="@+id/imageView2"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="150dp"
                        android:text="Actions: "
                        android:textSize="20sp"
                        app:backgroundTint="@color/purple_200"
                        app:cornerRadius="20dp" />

                    <TextView
                        android:id="@+id/textviewcommentaires2"
                        android:layout_width="wrap_content"
                        android:layout_height="52dp"
                        android:layout_below="@+id/imageView2"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="190dp"
                        android:text="Commentaires : Rien a signaler "
                        android:textSize="20sp"
                        app:backgroundTint="@color/purple_200"
                        app:cornerRadius="20dp" />

                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="46dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/purple_500"
                    android:gravity="center"
                    android:text="Infos Client"
                    android:textColor="#FFFFFF"
                    android:textSize="20dp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/grey"
                    android:layout_margin="10dp">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Nom Complet : Jean "
                    android:textColor="@color/black"
                    android:textSize="16sp" />
                    <TextView
                        android:layout_marginTop="30dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Titre:Societe Exemple "
                        android:textColor="@color/black"
                        android:textSize="16sp" />
                    <TextView
                        android:layout_marginTop="60dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Societe:Societe Exemple "
                        android:textColor="@color/black"
                        android:textSize="16sp" />
                    <TextView
                        android:layout_marginTop="90dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Appel : "
                        android:textColor="@color/black"
                        android:textSize="16sp" />
                    <TextView
                        android:layout_marginTop="120dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Téléphone : "
                        android:textColor="@color/black"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="341dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginTop="90dp"
                        android:gravity="center"
                        android:text="Jean@gmail.com"
                        android:textColor="#3F51B5"
                        android:textSize="16sp" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="120dp"
                        android:gravity="center"
                        android:text="12 345 678 "
                        android:textColor="#3F51B5"
                        android:textSize="16sp" />

                    <Button
                        android:layout_width="348dp"
                        android:layout_height="73dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="170dp"
                        android:drawableStart="@drawable/messages"
                        android:gravity="center"
                        android:text="Envoyer Un Rapport au client"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        app:cornerRadius="50dp" />
                </androidx.cardview.widget.CardView>


            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="46dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/purple_500"
                    android:gravity="center"
                    android:text="Adresse"
                    android:textColor="#FFFFFF"
                    android:textSize="20dp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/grey"
                    android:layout_margin="10dp">


                    <TextView
                        android:layout_marginTop="30dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Adresse: "
                        android:textColor="@color/black"
                        android:textSize="20sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="90dp"
                        android:layout_marginTop="30dp"
                        android:gravity="start"
                        android:text="Rue paradis"
                        android:textColor="#000000"
                        android:textSize="20sp" />

                    <Button
                        android:layout_width="137dp"
                        android:layout_height="63dp"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="80dp"
                        android:layout_marginBottom="20dp"
                        android:drawableStart="@drawable/adresse"
                        android:drawableLeft="@drawable/adresse"
                        android:gravity="center"
                        android:onClick="goMap"
                        android:text="Carte"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        app:cornerRadius="50dp" />
                </androidx.cardview.widget.CardView>


            </LinearLayout>
             <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="46dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/purple_500"
                    android:gravity="center"
                    android:text="Piece Jointe"
                    android:textColor="#FFFFFF"
                    android:textSize="20dp" />

                 <TextView
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_marginStart="10dp"
                     android:layout_marginTop="10dp"
                     android:text="https://stackoverflow.com/questions/62420160/send-intent-with-extrastring-from-activity-to-tab-fragment"
                     android:textColor="@color/purple_700"
                     android:textSize="16sp" />

             </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="46dp"
                    android:layout_marginTop="20dp"
                    android:background="@color/purple_500"
                    android:gravity="center"
                    android:text="Historique"
                    android:textColor="#FFFFFF"
                    android:textSize="20dp" />

                <Button
                    android:layout_width="149dp"
                    android:layout_height="71dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="20dp"
                    android:onClick="goHistorique"
                    android:text="Historique"
                    android:textColor="@color/white"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:cornerRadius="50dp" />

            </LinearLayout>



        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>


</ScrollView>